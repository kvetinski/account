{
  "id": null,
  "uid": "account-observability",
  "title": "Account Service Observability",
  "tags": ["account", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "gRPC RPS By Method",
      "id": 1,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(rate(account_grpc_requests_total[$__rate_interval])) by (method)",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC p95 Latency By Method",
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(account_grpc_request_duration_seconds_bucket[$__rate_interval])) by (le, method))",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC Responses By Code (RPS)",
      "id": 3,
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(rate(account_grpc_requests_total[$__rate_interval])) by (code)",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC In-Flight Requests",
      "id": 4,
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "account_grpc_requests_in_flight",
          "legendFormat": "in_flight",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "DB QPS By Method",
      "id": 5,
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(rate(account_db_queries_total[$__rate_interval])) by (method)",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "DB p95 Latency By Method",
      "id": 6,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(account_db_query_duration_seconds_bucket[$__rate_interval])) by (le, method))",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Go Runtime (Goroutines/Threads)",
      "id": 7,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "goroutines",
          "refId": "A"
        },
        {
          "expr": "go_threads",
          "legendFormat": "threads",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "App CPU Seconds / sec",
      "id": 8,
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[$__rate_interval])",
          "legendFormat": "app_cpu",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "App Memory RSS",
      "id": 9,
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "app_rss",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Go Heap Alloc",
      "id": 10,
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "go_memstats_heap_alloc_bytes",
          "legendFormat": "heap_alloc",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Container CPU / sec",
      "id": 11,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{name=~\"account-.*\"}[$__rate_interval])) by (name)",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Container Memory Working Set",
      "id": 12,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{name=~\"account-.*\"}) by (name)",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ]
    }
  ]
}
